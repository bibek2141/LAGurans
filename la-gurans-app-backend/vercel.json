{
  "version": 2,
  "builds": [
    {
      "src": "la-gurans-app-frontend/dist/la-gurans-app-frontend/**",
      "use": "@vercel/static-build"
    },
    {
      "src": "la-gurans-app-backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "la-gurans-app-backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "la-gurans-app-frontend/dist/la-gurans-app-frontend/browser/index.html"
    }
  ]
}
