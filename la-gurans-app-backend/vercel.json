{
  "version": 2,
  "builds": [
    {
      "src": "la-gurans-app-backend/package.json",
      "use": "@vercel/node"
    },
    {
      "src": "la-gurans-app-frontend/dist/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "la-gurans-app-backend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "la-gurans-app-frontend/dist/$1"
    }
  ]
}
